<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>WebToDB</servlet-name>
        <servlet-class>com.wdy.servlet.WebToDB</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WebToDB</servlet-name>
        <url-pattern>/WebToDB</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>WebToDB2</servlet-name>
        <servlet-class>com.wdy.servlet.WebToDB2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WebToDB2</servlet-name>
        <url-pattern>/WebToDB2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DBToPort</servlet-name>
        <servlet-class>com.wdy.servlet.DBToPort</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DBToPort</servlet-name>
        <url-pattern>/DBToPort</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowPre</servlet-name>
        <servlet-class>com.wdy.servlet.ShowPre</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowPre</servlet-name>
        <url-pattern>/ShowPre</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowPre2</servlet-name>
        <servlet-class>com.wdy.servlet.ShowPre2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowPre2</servlet-name>
        <url-pattern>/ShowPre2</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>./WEB-INF/index.html</welcome-file>
    </welcome-file-list>
</web-app>